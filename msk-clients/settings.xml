<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 https://maven.apache.org/xsd/settings-1.0.0.xsd">
    <localRepository/>
    <interactiveMode/>
    <offline/>
    <pluginGroups/>
    <servers>
        <server>
            <id>schema-artifact-domain-schema-artifactory</id>
            <username>aws</username>
            <password>${env.CODEARTIFACT_AUTH_TOKEN}</password>
        </server>
    </servers>
    <mirrors/>
    <proxies/>
    <profiles>
        <profile>
            <id>schema-artifact-domain-schema-artifactory</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <repositories>
                <repository>
                    <id>schema-artifact-domain-schema-artifactory</id>
                    <url>https://schema-artifact-domain-<<AWS_ACCOUNT_ID>>.d.codeartifact.<<AWS_REGION>>.amazonaws.com/maven/schema-artifactory/</url>
                </repository>
            </repositories>
        </profile>
    </profiles>
    <activeProfiles/>
</settings>